<div id="heading">
    <h1> Sign In</h1>
</div>

<div id="form-container">

<form action="/users/create-session" method="post">

    <input type="email" name="email" placeholder="Email Id" required>
    <input type="password" name="password" placeholder="Enter password" required>
    <input type="submit" value="Sign In">
</form>
<div id="google-sign">
        
    <div>
        <img src="/images/google-brands.svg">
    </div>
    
    <div>
        <a href="/users/auth/google">Sign In with Google!</a>
    </div>
    

</div>
</div>

<script src="https://kit.fontawesome.com/9863b744e9.js" crossorigin="anonymous"></script>